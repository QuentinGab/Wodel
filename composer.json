{
    "name": ":vendor/wodel",
    "type": "library",
    "description": "WordPress Post Model",
    "keywords": [
        ":vendor",
        "wodel"
    ],
    "homepage": "https://github.com/:vendor/wodel",
    "license": "MIT",
    "authors": [
        {
            "name": "quentin gabriele",
            "email": "quentin.gabriele@gmail.com",
            "homepage": "https://linkedin.com/in/quentin-gabriele-86b161196/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            ":vendor\\wodel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\wodel\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
